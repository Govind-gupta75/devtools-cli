{
  "name": "@adobe/uxp-devtools-app",
  "productName": "Adobe UXP Developer Tool",
  "appMenuName": "Adobe UXP Developer Tool",
  "version": "1.2.0",
  "main": "main/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe-uxp/devtools-cli.git",
    "directory": "packages/uxp-devtools-app"
  },
  "author": "Adobe Inc",
  "license": "Apache-2.0",
  "scripts": {
    "cleanup": "rimraf dist/",
    "start-main-dev": "cross-env NODE_ENV=development electron ./main/index.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode=development",
    "build-renderer": "yarn cleanup && cross-env NODE_ENV=production webpack",
    "package": "yarn build-renderer && electron-builder --dir",
    "cdt-custom-setup": "node scripts/cdt_inspect/cdtInspectPreSetup.js",
    "postinstall": "yarn cdt-custom-setup &&  node scripts/installAppDeps.js"
  },
  "build": {
    "productName": "Adobe UXP Developer Tool",
    "removePackageScripts": "false",
    "appId": "com.adobe.uxp.devtools.app",
    "files": [
      "dist/",
      "main/",
      "!node_modules/@adobe/uxp-devtools-helper/scripts${/*}"
    ],
    "mac": {
      "target": "dir",
      "icon": "resources/icons/app/icon.icns",
      "category": "public.app-category.developer-tools",
      "identity": null
    },
    "win": {
      "target": "dir",
      "icon": "resources/icons/app/icon.ico",
      "signAndEditExecutable": true
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "autoprefixer": "^8.6.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.6.0",
    "electron": "8.2.0",
    "electron-builder": "^22.7.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-rebuild": "^1.11.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-loader": "^1.0.1",
    "node-sass": "^4.14.1",
    "postcss-loader": "^2.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-md5-hash": "^0.0.6",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^2.5.0"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.3.0",
    "@adobe/uxp-devtools-core": "^1.2.0",
    "@adobe/uxp-template-default-starter": "^1.0.0",
    "@adobe/uxp-template-default-starter-ps": "^1.0.0",
    "@adobe/uxp-template-default-starter-xd": "^1.0.0",
    "@adobe/uxp-template-ps-react-starter": "^1.0.0",
    "@adobe/uxp-template-ps-react-typescript": "^1.0.0",
    "@adobe/uxp-template-ps-starter": "^1.0.0",
    "@adobe/uxp-template-xd-starter": "^1.0.0",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@react-spectrum/table": "3.0.0-alpha.6",
    "@spectrum-icons/workflow": "^3.0.2",
    "chalk": "^4.1.0",
    "chrome-devtools-frontend": "1.0.672485",
    "classnames": "^2.2.6",
    "events": "^3.2.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.19",
    "minimist": "^1.2.5",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.12.0",
    "source-map-support": "^0.5.16",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  }
}
